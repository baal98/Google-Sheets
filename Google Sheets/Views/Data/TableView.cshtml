@model List<CreateTableModel>

<table>
    <thead>
        <tr>
            @foreach (var columnName in Model)
            {
                <th>@columnName.SpreadsheetId</th>
                <th>@columnName.NumberOfColumns</th>
                <th>@columnName.TableName</th>
                <th>@columnName.Description</th>
            }
        </tr>
    </thead>
    <tbody>
        <!-- Add your table rows here based on the data you have -->
    </tbody>
</table>